{
  "rewrites": [
    {
      "source": "/tutor",
      "destination": "/tutor.html"
    },
    {
      "source": "/reference",
      "destination": "/reference.html"
    },
    {
      "source": "/calendar",
      "destination": "/calendar_study_system.html"
    },
    {
      "source": "/study-helper",
      "destination": "/web_study_helper.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
